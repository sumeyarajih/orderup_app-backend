generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           Int      @id @default(autoincrement())
  fullName     String
  email        String   @unique
  password     String
  phoneNumber  String   @unique
  otp          String?
  otpExpiresAt DateTime?
  createdAt    DateTime @default(now())
}
